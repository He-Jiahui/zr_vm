// 测试 class prototype 生成
module "test_class";

// 基本 class 声明
class Animal {
    pub var name: string;
    
    pub speak() {
        return "Some sound";
    }
}

// 带继承的 class
class Dog : Animal {
    pub speak() {
        return "Woof";
    }
}

// 带属性的 class
class Person {
    pub var _name: string;
    
    pub get name: string {
        return this._name;
    }

    pub set name(v: string){
        this._name = v;
    }
}

// 带元函数的 class
class TestClass {
    pub var value: int;
    

    pub @constructor(v: int) {
        value = v;
    }
    

    pub @destructor() {
        value = 0;
    }
}


